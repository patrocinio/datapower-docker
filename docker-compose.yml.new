version: '2'
services:
  web:
    ports:
     - 9090:9090
    image: eduardo-datapower
    environment:
     - DATAPOWER_ACCEPT_LICENSE=true
     - DATAPOWER_INTERACTIVE=true
    volumes:
      - config:/drouter/config
      - local:/drouter/local

volumes:
  config:
    external:
      name: datapower_config
  local:
    external:
      name: datapower_local
